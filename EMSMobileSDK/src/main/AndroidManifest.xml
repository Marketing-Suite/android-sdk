<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="experian.mobilesdk">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">
        <service
            android:name=".MessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>

        <service
            android:name=".IDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

        <receiver android:name="NotificationReceiver" android:enabled="true">
            <intent-filter>
                <action android:name="com.experian.mobilesdk.EMS_SHOW_NOTIFICATION" />
                <action android:name="com.experian.mobilesdk.EMS_OPEN_NOTIFICATION" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>
    </application>
</manifest>
